  "author": "Michael Hart <mhart@cloudflare.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "LICENSE",
    "README.md"
  ]
